{
  "name": "@angular-monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx nx run-many -t build",
    "docker-build:admin-dashboard": "cd devops/admin-dashboard && docker build --tag money-split-admin-dashboard .",
    "docker-build:data-provider": "cd devops/data-provider && docker build --tag money-split-data-provider .",
    "docker-build:frontend": "cd devops/frontend && docker build --tag money-split-frontend .",
    "docker-build:migration-manager": "cd devops/migration-manager && docker build --tag money-split-migration-manager .",
    "docker-compose:production-local-up": "cd devops && docker-compose --env-file .production-local.env up",
    "docker-compose:production-local": "npx nx run-many -t build && npm run docker-build:admin-dashboard && npm run docker-build:data-provider && npm run docker-build:frontend && npm run docker-build:migration-manager && npm run docker-compose:production-local-up",
    "test:data-provider": "npx nx run data-provider-e2e:e2e --configuration=development --skip-nx-cache",
    "test:development:headless": "nx run debt-simplification:test:ci --skip-nx-cache && nx run data-provider-e2e:e2e --configuration=ci --skip-nx-cache --parallel && nx run money-split-e2e:e2e:development --skip-nx-cache",
    "test:production-local:headless": "nx run data-provider-e2e:e2e --configuration=production-local --skip-nx-cache --parallel && nx run money-split-e2e:e2e:production-local --skip-nx-cache"
  },
  "private": true,
